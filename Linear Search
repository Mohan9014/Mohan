#include<iostream>
using namespace std;

int main(){
     int arr[] = {10, 20, 30, 40, 50};
    int val=30;
    
    int index=-1;
    int n= sizeof(arr)/sizeof(arr[0]);
    
    for(int i=0;i<n;i++){
        if(arr[i]==val){
            index=i;break;
            
        }
    }
    if(index==-1){
        cout << "not found" <<endl;
        
    }
    else{
        cout << "found at index" << index;
    }
}
